<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global">
    <!-- Attributes -->
    <aura:attribute name="listOppLineItem" type="List" default="['a1','a2','a3','a4','a5','a6','a7','a8','a9','a10']" />
    <aura:attribute name="listOppLineItem2" type="List" default="['b1','b2','b3','b4','b5','b6','b7','b8','b9','b10']" />
    <aura:attribute name="listOppLineItem3" type="List" default="['c1','c2','c3','c4','c5','c6','c7','c8','c9','c10']" />
    <aura:attribute name="listOppLineItem4" type="List" default="['d1','d2','d3','d4','d5','d6','d7','d8','d9','d10']" />
    <aura:attribute name="listOppLineItem5" type="List" default="['e1','e2','e3','e4','e5','e6','e7','e8','e9','e10']" />

    <div clbss="main-container">
        <lightning:card title="Escolha seus Assentos">

            <lightning:layout horizontalAlign="space">
                <aura:iteration items="{!v.listOppLineItem}" var="oppLineItem">
                    <div class="fileira">
                        <lightning:layoutItem padding="around-small">
                            <!-- Adicione um aura:id único com base no oppLineItem -->
                            <div class="custom-box" onmouseenter="{!c.seatHover}" aura:id="{!'cadeira_' + oppLineItem}">
                            </div>
                        </lightning:layoutItem>
                    </div>
                </aura:iteration>
            </lightning:layout>
            

            <lightning:layout horizontalAlign="space">
                <aura:iteration items="{!v.listOppLineItem2}" var="oppLineItem">
                    <div class="fileira">
                        <lightning:layoutItem padding="around-small">
                            <!-- Adicione um aura:id único com base no oppLineItem -->
                            <div class="custom-box" onmouseenter="{!c.seatHover}" aura:id="{!'cadeira_' + oppLineItem}">
                            </div>
                        </lightning:layoutItem>
                    </div>
                </aura:iteration>
            </lightning:layout>


            <lightning:layout horizontalAlign="space">
                <aura:iteration items="{!v.listOppLineItem3}" var="oppLineItem">
                    <div class="fileira">
                        <lightning:layoutItem padding="around-small">
                            <!-- Adicione um aura:id único com base no oppLineItem -->
                            <div class="custom-box" onmouseenter="{!c.seatHover}" aura:id="{!'cadeira_' + oppLineItem}">
                            </div>
                        </lightning:layoutItem>
                    </div>
                </aura:iteration>
            </lightning:layout>

            <lightning:layout horizontalAlign="space">
                <aura:iteration items="{!v.listOppLineItem4}" var="oppLineItem">
                    <div class="fileira">
                        <lightning:layoutItem padding="around-small">
                            <!-- Adicione um aura:id único com base no oppLineItem -->
                            <div class="custom-box" onmouseenter="{!c.seatHover}" aura:id="{!'cadeira_' + oppLineItem}">
                            </div>
                        </lightning:layoutItem>
                    </div>
                </aura:iteration>
            </lightning:layout>

            <lightning:layout horizontalAlign="space">
                <aura:iteration items="{!v.listOppLineItem5}" var="oppLineItem">
                    <div class="fileira">
                        <lightning:layoutItem padding="around-small">
                            <!-- Adicione um aura:id único com base no oppLineItem -->
                            <div class="custom-box" onmouseenter="{!c.seatHover}" aura:id="{!'cadeira_' + oppLineItem}">
                            </div>
                        </lightning:layoutItem>
                    </div>
                </aura:iteration>
            </lightning:layout>

            <!-- Outros layouts e iterações aqui -->

            <div class="tela">
                TELA
            </div>
        </lightning:card>
    </div>
</aura:component>